    "import time\n",
    "counter = 1\n",
    "table_number = int(input(\"Enter a value to calculate the table: \"))\n",
    "\n",
    "while counter <= 10:\n",
    "    print(table_number, \"x\", counter, \"=\", table_number * counter)\n",
    "    counter += 1\n",
    "    time.sleep(1)"
   ]
